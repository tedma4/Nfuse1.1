<!DOCTYPE html>
<!-- *Start* Views/layouts/application.html -->
<html>
  <head>
    <% if content_for? :for_head %>
      <%= yield :for_head %>
    <% end %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <title><%= full_title(yield(:title)) %></title>
    <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %> 
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %> 
    <%#= javascript_include_tag '//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js' %>
    <script async src="https://www.google.com/jsapi" type="text/javascript"></script>
    <script async src="https://www.youtube.com/iframe_api"></script>
    <%= javascript_include_tag 'masonry.pkgd.js' %>
    <%#= javascript_include_tag 'resize_masonry.js', async: true %>
    <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600' rel='stylesheet' type='text/css'>
    <%= favicon_link_tag 'Nfuselogopurple.fw.png'%>
    <%= render 'layouts/shim' %>
    <%= csrf_meta_tags %>
  </head>
  <body id="nfuse" class="loggedout inner pagetrans" data-spy="scroll" data-target=".navbar-fixed-top">
    
    <%= render 'layouts/header' %>
    <div class="">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>">
          <button type="button" class="close" data-dismiss="alert">&times;</button>
          <%= value %>
        </div>
      <% end %>
      <div class="">
        <%= yield %>
      </div>
    </div>

  <%= render partial: 'layouts/footer' %>
  <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
  <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
  <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
  <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
  <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
  <%= javascript_include_tag 'imagesloaded.pkgd.js' %>
  <script src="js/jquery.js"></script>
  <!-- Bootstrap Core JavaScript -->
  <script src="js/bootstrap.min.js"></script>
  <!-- Plugin JavaScript -->
  <script src="js/jquery.easing.min.js"></script>
  <!-- Custom Theme JavaScript 
  <script src="js/application.js"></script>-->
  <script src="js/nfuse.js"></script>  </body>
</html>
