<div class="individual_profile col-xs-6 col-sm-4 col-md-3 col-lg-2">
  <a href="/pages/<%= page.page_name %>">
    <div class="profile-head">
      <p><%= truncate(page.full_name, length: 18, omission: '') %></p>
    </div>
    <%#= page[:page].profile_pic ? image_tag(page[:page].profile_pic) : image_tag("FargoFX.jpg") %>
    <%= image_tag(page.profile_pic || "def.jpg")%>
  </a>
</div>