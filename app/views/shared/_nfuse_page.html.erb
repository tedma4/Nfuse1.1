<div id="masonJar" class="center hub_container clearfix masonJar">
  <% @nfuse_pages.each do |shout| %>
      <%#= shout.inspect %>
      <% if !shout.url.blank? %>
        <div class="individual_post faved_post item" id="content-box-feed">
          <p> 
            <%= shout.url_html %>
          </p>
        </div>
      <% elsif shout.from_youtube? %>
        <div class="individual_post faved_post item" id="content-box-feed">
          <iframe 
            id="ytplayer" 
            type="text/html" 
            width="100%" 
            height="225" 
            src="http://www.youtube.com/embed/<%= shout.url %>" 
            frameborder="0">
          </iframe>
        </div>
      <% else shout.is_pic? %>
        <div class="individual_post faved_post item" id="content-box-feed">
          <p class="timeline-user-box">
            <%= image_tag shout.url %>
          </p>
        </div>
      <% end %>
  <% end %>
</div>
