<%# byebug %>

<% if @pages.next_page %>
	<% @pages.each do |page| %>
	  $('#profile-grid').append('<%= j render partial: "pages/page", locals: {page: page} %>')
  <% end %>
	<% @pages.each do |page| %>
	  $('.pagination').replaceWith('<%= j render partial: "pages/page", locals: {page: page} %>')
  <% end %>
<% else %>
  $('.pagination').remove()
<% end %>