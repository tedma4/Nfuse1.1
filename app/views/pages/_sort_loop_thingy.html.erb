<% this_is_it = m.sort_by {|a, b| -Impression.where(action_name: a.to_s, user_id: current_user.id).count} %>
<% this_is_it.each do |this| %>
  <% page = Page.find_by_page_name(this) %>
  <!-- PROFILE -->
  <div class="individual_profile col-xs-6 col-sm-4 col-md-3 col-lg-2">
    <a href="/pages/<%= page.page_name %>">
      <div class="profile-head">
        <p><%= page.thing_name %></p>
      </div>
      <%= image_tag("#{page.twitter_handle}.jpg") %>
    </a>
  </div>
<% end %>
<!-- REQUEST ADDITIONAL -->    
<div id="request" class="individual_profile col-xs-12 col-sm-8 col-md-6 col-lg-">
  <div class="vcenter">
    <div class="vcell">
      <button type="button" class="btn btn-default default" data-toggle="modal" data-target="#requestmodal">Request Missing</button>
    </div>
  </div>
</div>