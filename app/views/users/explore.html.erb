
<div class="center block explore_container clearfix mosaicflow">
<% @timeline.each do |post| %>
    <%= render partial: "twitter_post", locals: {post: post} if post.is_a? Twitter::Post %>
    <%= render partial: "instagram_post", locals: {post: post} if post.is_a? Instagram::Post %>
    <%= render partial: "users/shout", locals: {shout: post } if post.is_a? Nfuse::Post %>
	<%= render partial: "facebook_post", locals: {post: post, poster_recipient_profile_hash: @poster_recipient_profile_hash, commenter_profile_hash: @commenter_profile_hash} if post.is_a? Facebook::Post %>
<% end %>
</div>
<!-- *End* Views/users/explore.html.erb  -->