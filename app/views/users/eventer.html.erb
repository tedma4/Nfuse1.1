<div class="subNav">
<ul class="subNavList">
	<li>        
	  <%= link_to hub_user_path(session[:user_id]) do %>
	    <%= image_tag('molecule4.png', size: "32x32") %>
	  <% end %>
	  hub
	</li>
	<li>        
	  <%= link_to explore_user_path(session[:user_id]) do %>
	    <%= image_tag('educational27.png', size: "32x32") %>
	  <% end %>
	  explore
	</li>
</ul>
</div>
<div class="center block hub_container clearfix mosaicflow">
<% @timeline.each do |post| %>
    <%= render partial: "users/event", locals: {event: post } if post.is_a? Nfuse::Post %>
<% end %>
</div>
<!-- *End* Views/users/hub.html.erb  -->